<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ez.gw.commute.model.CommuteDAO">

	<select id="selectCommuteByEmpNo" parameterType="int" resultType="commuteVo">
		select * from commute
		where emp_no=#{empNo}
	</select>
	
	<insert id="insertWorkIn" parameterType="int">
	    insert into commute(commute_no, work_in, commute_state, emp_no)
	    values(commute_seq.nextval, to_char(sysdate, 'yyyy-mm-dd hh24:mi:ss'), 0, #{empNo})
	</insert>
	
	
	
</mapper>