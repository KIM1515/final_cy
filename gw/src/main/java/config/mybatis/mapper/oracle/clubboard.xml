<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ez.gw.clubboard.model.ClubBoardDAO">

	<select id="selectClubBoard" resultType="clubboardVo" parameterType="int">
		select * from clubBoard
		where CLUB_NO=#{clubNo}
	</select>
	
	<insert id="insertClubBoard" parameterType="clubBoardVo">
		<selectKey keyProperty="boardNo" order="BEFORE" resultType="int">
			select clubboard_seq.nextval from dual
		</selectKey>
		insert into clubBoard(BOARD_NO, CLUB_NO, EMP_NO, TITLE, CONTENT, BOARDLIST_NO,DELFLAG)
		values(#{boardNo}, #{clubNo}, #{empNo}, #{title}, #{content},200,#{delflag})
	</insert>
	
	
	
</mapper>























